    "react-spring": "^9.7.3",
    "react-use-gesture": "^9.1.3",
    "swiper": "^11.0.0",
    "three": "^0.158.0",
    "web-vitals": "^3.5.0",
    "workbox-window": "^7.0.0",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
